{
    "name": "90zlaya/php-library",
    "type": "library",
    "description": "PHP Library is a set of classes that contain the most useful attributes and methods that faciliate the development of Web applications",
    "keywords": [
        "php",
        "library",
        "oop",
        "php-library"
    ],
    "homepage": "https://github.com/90zlaya/php-library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zlatan Stajic",
            "email": "contact@zlatanstajic.com",
            "homepage": "https://www.zlatanstajic.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "PHP_Library\\League\\Data\\": "src\\data",
            "PHP_Library\\League\\Files\\": "src\\files",
            "PHP_Library\\League\\Format\\": "src\\format",
            "PHP_Library\\League\\Math\\": "src\\math",
            "PHP_Library\\League\\SQL\\": "src\\sql",
            "PHP_Library\\League\\Services\\": "src\\services"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "phpoffice/phpspreadsheet": "1.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.*",
        "phpstan/phpstan": "0.9.*",
        "squizlabs/php_codesniffer": "3.4.*"
    }
}
